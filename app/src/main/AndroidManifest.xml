        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CoursesList" />
        <activity android:name=".NameCourse" />
        <activity android:name=".Organization" />
        <activity android:name=".RiyadhActivity" />
        <activity android:name=".DammamActivity" />
        <activity android:name=".JeddahActivity"></activity>
    </application>

</manifest>